<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.golfcaddy"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="7" />

    <application
        android:debuggable="true"
        android:icon="@drawable/golf_caddy"
        android:label="@string/app_name" >
        <activity
            android:name="com.example.golfcaddy.activities.GolfCaddy"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Light" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.golfcaddy.activities.Preferences" 
             android:theme="@android:style/Theme.Light">
        </activity>
        <activity android:name=".activities.About" 
             android:theme="@android:style/Theme.Light">
        </activity>
    </application>

</manifest>